var slashPos = application.name.indexOf("/");
gAppName = application.name.slice(0, slashPos);
gCustomerName = application.name.slice(slashPos + 1);

// _definst_ is used for the tutor; any other instance is a student
if (gCustomerName == "_definst_") {
	load("tutor.asc");
} else {
	load("student.asc");
}